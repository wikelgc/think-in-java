			return 
					"value1 = "+ "value1"+" "+
					"value2 = "+"value2"+" "+
					"value3 = "+ "value3"+""+
					"i = " + i+ " " + "f="+f+""+
					"source =" + source; 
		}

		public static void main(String[] args){
		SprinklerSystem sprinlers = new SprinklerSystem();
		System.out.println(sprinlers);
		}

public class WaterSource {
	private  String s;
	WaterSource(){
		System.out.println("waterSource()");
		s= "Construted";
	}
	public String toString(){
		return s;
	}
}
	
	public class SprinklerSystems{
		private String valve1,valve2,valve3;
		private WaterSource source = new WaterSource();
